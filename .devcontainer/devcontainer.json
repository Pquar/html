{
    "name": "Mirror Fashion - PHP & MySQL",
    "dockerComposeFile": "docker-compose.yml",
    "service": "app",
    "portsAttributes": {
        "3000": {
            "label": "Web Application",
            "onAutoForward": "openPreview"
        },
        "8080": {
            "label": "phpMyAdmin",
            "onAutoForward": "ignore"
        },
        "3306": {
            "label": "MySQL",
            "onAutoForward": "ignore"
        }
    },
    "forwardPorts": [3000, 8080, 3306],
    "postCreateCommand": "composer install",
    "remoteUser": "www-data",
    "containerEnv": {
        "SHELL": "/bin/bash"
    }
}